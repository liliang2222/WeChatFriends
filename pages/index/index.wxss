/**index.wxss**/
page{
  background-color:#F8F8F8;
}
.content{
  width: 100%;
}
.describe {
  width: 100%;
  padding: 10px 16px;
  background-color: white;
}
.describe textarea{
  width: 100%;
  font-size: 14px;
}
.img_layout{
  width: 100%;
  float: left;
}
.img{
  position: relative;
  width: 33.3333%;
  float: left;
  box-sizing: border-box;
  padding: 10px;
}
.img_icon{
  position: relative;
  padding: 0px 0px;
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}
.img image{
  height: 100px;
  width: 100px;
}


